{
	"packages": [
		"packages/*"
	],
	"version": "independent",
	"command": {
		"publish": {
			"allowBranch": [
				"master",
				"release/**"
			],
			"message": "chore(release): publish"
		}
	},
	"changelog": {
		"repo": "TOC-TEAM/lernaReposTest",
		"labels": {
			"enhancement": ":rocket: Enhancement",
			"bug": ":bug: Bug Fix",
			"doc": "Refine Doc",
			"feat": "New Feature"
		},
		"cacheDir": ".changelog"
	}
}